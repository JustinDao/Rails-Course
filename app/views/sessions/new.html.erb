<h1>Log in</h1> 
<div>
  <%= form_for(:session, url: create_session_path) do |f| %> 
    <%= f.label :email %> 
    <%= f.text_field :email %> 
    <%= f.label :password %> 
    <%= f.password_field :password %> 
    <%= f.submit "Sign in" %>
  <% end %> 
  <p>
    New user? 
    <%= link_to "Sign up!", sign_up_path %>
  </p> 
</div> 
